                    "properties": {
                      "id": { "type": "integer", "description": "Id du journal" },
                      "description": { "type": "string", "description": "Description du journal" },
                      "apprenticeId": {
                        "type": "integer",
                        "description": "Id de l'apprenti lié au journal"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "get": {
